<h1>Listing parts</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Description</th>
    <th>Part type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @parts.each do |prt| %>
      <tr>
        <td><%= prt.id %></td>
        <td><%= prt.name %></td>
        <td><%= prt.description %></td>
        <td><%= prt.part_type.type_name %></td>
        <td><%= link_to 'Show', prt %></td>
        <td><%= link_to 'Edit', edit_part_path(prt) %></td>

      </tr>
  <% end %>
</table>

<br />

<%= link_to 'New Part', new_part_path %>
